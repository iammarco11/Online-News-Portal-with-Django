{
    "builds": [{
        "src": "newsportal/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "100mb" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "newsportal/wsgi.py"
        }
    ],
    "functions": [{
        "*": {
            "excludeFiles": "media/**"
        }
    }]
}
